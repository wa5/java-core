package com.company;

import javax.print.Doc;

class EkClass{
    int a;

    public int getA() {
        return a;
    }

    EkClass(int a){
        this.a = a;
    }
    public int returnone(){
        return 1;
    }
}
class DoClass extends EkClass{
    DoClass(int c){
        super(c);
        System.out.println("I am a constructor");
    }
}
public class cwh_47_this_super {
    public static void main(String[] args) {
        EkClass e = new EkClass(65);
        DoClass d = new DoClass(5);
        System.out.println(e.getA());
    }
}

final key word

//final class VIshnu2 {// no one can  extend
class VIshnu2 {
//final void m1() {//no one can ovverride
	void m1() {
		 System.out.println("im vishnu method");
	}
}

