
abstract class Vahicle2 {

	 
	 abstract void start();
	}

 class Car extends Vahicle2{

	 void start() {
		 System.out.println("starts with key ");
	 }
	}

 class Scooter extends Vahicle2{

	 void start() {
		 System.out.println("starts with kick ");
	 }
	}
public class VIshnu1 {

